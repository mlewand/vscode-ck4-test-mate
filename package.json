{
  "name": "ck4-test-mate",
  "displayName": "ck4-test-mate",
  "description": "Helper for CKEditor 4 test development.",
  "version": "0.0.1",
  "publisher": "mlewandowski",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ck4-test-mate.openTest"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "ck4-test-mate.openTest",
        "title": "Open test in your browser"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "opn": "^4.0.2"
  }
}
